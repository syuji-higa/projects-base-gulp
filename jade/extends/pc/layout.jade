extends /extends/layout

//- append vars

block html
  doctype html
  html(lang="ja")
    head
      block head
        block metas
          include /includes/metas
          link(rel="shortcut icon", href=favicon)
        block styles
          link(rel="stylesheet", href="/styles/style.css", media="all")
        block scripts
          script(src="/scripts/index.js")
    body&attributes(bodyAttrs)
      block body
        .wrap
          block header
            header
          block contents
          block footer
            footer
