{
  "parser": "babel-eslint",
  "env": { "browser": true, "node": true, "es6": true },
  "rules": {
    "comma-dangle": 0,
    "constructor-super": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-negated-in-lhs": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assignL": 0,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 1,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 1,
    "use-isnan": 2,
    "valid-typeof": 2,
    "semi": 2,
    "camelcase": 2,
    "indent": [2, 2],
    "no-lonely-if": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "semi-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "wrap-regex": 2,
    "block-spacing": 1,
    "comma-style": 1,
    "consistent-this": 1,
    "eol-last": 1,
    "arrow-parens": [2, "always"],
    "arrow-spacing": 2,
    "generator-star-spacing": [2, {"before": true, "after": false}],
    "no-var": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-yield": 2
  }
}
